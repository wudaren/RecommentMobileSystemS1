<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_dianyuanlist_1210"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/tv_dianyuanyilanid_1210"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:gravity="center"
        android:textColor="@color/white"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_qnumber_1210"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        android:minHeight="40dp"
        android:textColor="@color/listitem" />

    <TextView
        android:id="@+id/tv_xingming_1210"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="3.5"
        android:gravity="center_horizontal|left"
        android:layout_marginLeft="10dp"
        android:minHeight="40dp"
        android:maxWidth="80dp"
        android:textColor="@color/listitem" />

    <TextView
        android:id="@+id/tv_dengluming_1210"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="3.5"
        android:gravity="center_horizontal|left"
        android:maxWidth="80dp"
        android:minHeight="40dp"
        android:layout_marginLeft="10dp"
        android:textColor="@color/listitem" />

    <TextView
        android:id="@+id/tv_quanxian_1210"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:gravity="center"
        android:minHeight="40dp"
        android:textColor="@color/listitem" />

    <TextView
        android:id="@+id/tv_dianhua_1210"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:gravity="center"
        android:minHeight="40dp"
        android:textColor="@color/listitem"
        android:visibility="gone" />

</LinearLayout>